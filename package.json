{
  "name": "penni",
  "version": "1.0.0",
  "description": "",
  "main": "dist/server.js",
  "scripts": {
    "client": "cd client && npm start",
    "client:build": "cd client && npm run build",
    "start:dev": "tsc && concurrently --kill-others-on-fail \"tsc -w\" \"nodemon dist/server.js\"",
    "start": "tsc && node dist/server.js",
    "dev": "concurrently --kill-others-on-fail \"npm run client\" \"npm run server:dev\"",
    "test": "ENV_FILE=.env.test npx jest --runInBand --detectOpenHandles --colors --verbose --reporters=default",
    "test:ci": "npx jest --ci --runInBand --silent --colors --coverage",
    "lint": "npx prettier --check .",
    "format": "npx prettier --write ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nhsufi/Penni.git"
  },
  "author": "Naveed Sufi",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/nhsufi/Penni/issues"
  },
  "homepage": "https://github.com/nhsufi/Penni#readme",
  "dependencies": {
    "@types/cors": "^2.8.8",
    "@types/node": "^14.14.6",
    "body-parser": "^1.19.0",
    "concurrently": "^5.3.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-jwt": "^6.0.0",
    "jwks-rsa": "^1.11.0"
  },
  "devDependencies": {
    "@types/jest": "^26.0.15",
    "@types/supertest": "^2.0.10",
    "jest": "^26.6.3",
    "nodemon": "^2.0.6",
    "prettier": "2.1.2",
    "supertest": "^6.0.1",
    "ts-jest": "^26.4.3",
    "typescript": "^4.0.5"
  }
}
